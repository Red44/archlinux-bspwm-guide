!! URxvt Appearance
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.geometry: 92x24
URxvt.internalBorder: 24
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 4096
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
URxvt.iso14755: false
URxvt.shading : 15
URxvt.font: xft:Source Code Pro-10:pixelsize=13,\xft:Inconsolata for Powerline:pixelsize=12\
URxvt.depth: 32
URxvt.transparent: false
URxvt.imLocale :  en_US.UTF-8
URxvt.termName : rxvt-unicode-256color
URxvt.intensityStyles : false


!! Copy Paste & Other Extensions
URxvt.perl-ext-common: default,matcher,fullscreen,selection-to-clipboard
URxvt.keysym.F11: perl.fullscreen switch
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.urlLauncher: firefox-develper-edition
URxvt.underlineURLs: true
URxvt.urlButton: 1
